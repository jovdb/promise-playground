{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es5",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitThis": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"lib": [
			"es2017",
			"dom"
		],
		"strict": true
	},
}